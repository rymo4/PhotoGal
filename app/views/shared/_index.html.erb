<h1>Listing photos</h1>

<table>
  

<% @photos.each do |photo| %>
	<tr>
		<th><%= photo.title %></th>
	</tr>
  <tr>
    
    <td><center><%= link_to image_tag (photo.image_url(:thumb).to_s), photo %></center></td>
	</tr>
	<td>
		<%if photo.user_id==current_user.id%>
			<%= link_to 'Edit', edit_photo_path(photo) %> 
			<%= link_to 'Delete', photo, :confirm => 'Are you sure?', :method => :delete %></td>
		<%end%>
  </td>
   
  </tr>
<% end %>
</table>

<br />

