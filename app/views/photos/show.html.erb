<% in signed_in?%>
<p id="notice"><%= notice %></p>
<center>
<h1><%= @photo.title %></h1>
<%= image_tag @photo.image_url.to_s %>



<p>
  Taken by <%= link_to User.find(@photo.user_id).first_name, User.find(@photo.user_id) %>.
</p>


  

<p>
  
  <%= @photo.comment %>
</p>
</center>

<%= link_to 'Edit', edit_photo_path(@photo) %> |
<%= link_to 'Back', photos_path %>
<%else%>
<%= render 'shared/signin'%>
<%end%>
